datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  googleId  String   @unique
  email     String?  @unique
  name      String?
  picture   String?
  createdAt DateTime @default(now())
}
